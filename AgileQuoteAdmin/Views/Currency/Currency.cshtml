<!DOCTYPE html>
<html dir="ltr" lang="en">
<head>

    <title>Agile Quote</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="Shortcut Icon" type="image/ico" href="" />
    <script src="../../Scripts/jquery-1.7.1.js" type="text/javascript"></script>
    <script src="../../Scripts/CurrencyScript/CurrencyScript.js" type="text/javascript"></script>


    <!--[if lt IE 9]><script src="js/html5.js"></script><![endif]-->
</head>




<body>
    <section class="c12 main-title">
        <div class="c6 quote">
            <img src="../../Images/qoute-ion.png" alt="img1" />
            <p>Currency Form</p>


        </div>
    </section>
    @using (Html.BeginForm("Currency", "Currency", FormMethod.Post))
    {
        <div class="greay_box greay_box_currency">
            <ul>
                <li>
                    @Html.LabelFor(x => x.CurrencyCode, new { @class = "Label" })
                    @Html.TextBoxFor(x => x.CurrencyCode, new { @id = "CurrencyCode", @class = "TextBox" })
                    @Html.ValidationMessageFor(x => x.CurrencyCode)
                </li>
                <li>
                    @Html.LabelFor(x => x.CurrencyName, new { @class = "Label" })
                    @Html.TextBoxFor(x => x.CurrencyName, new { @class = "TextBox" })
                    @Html.ValidationMessageFor(x => x.CurrencyName)
                </li>
                <li>
                    @Html.LabelFor(x => x.Amount, new { @class = "Label" })
                    @Html.TextBoxFor(x => x.Amount, new { @class = "TextBox" })
                    @Html.ValidationMessageFor(x => x.Amount)
                    @*@Html.ValidationSummary(true)*@
                </li>

            </ul>
        </div>
                      
                       
                    
        <div class="submit_box submit-box-create">
            <input type="submit" value="Create Currency" />
        </div>               
    }
    </div>
            </section>
           
            <section>

                <div class="quote_list">
                </div>
            </section>


</body>
</html>


