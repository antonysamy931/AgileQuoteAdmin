                </div>
            </section>
            
@using (Html.BeginForm("RoleList", "Role", FormMethod.Post))
{
    <section>
     <div class="greay_box">
    <ul>
       <li>
        @Html.LabelFor(x => x.RoleName)
        @Html.TextBoxFor(x => x.RoleName)
        @Html.ValidationMessageFor(x => x.RoleName)
       </li>
       
    
        <li>
        @Html.LabelFor(x => x.Priority)
        @Html.TextBoxFor(x => x.Priority)
        @Html.ValidationMessageFor(x => x.Priority)
        </li>
     </ul>     
        </div>
        </section>
        @Html.ValidationSummary(true)
        <div class="submit_box">
       <input type="submit" value="Create" class="Button" />
        </div>
    }
   
  
   

</body>
