@model AgileQuoteAdminProperty.Property.RentalSparsProperty

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>RentalSpars</title>
    <script src="../../Scripts/jquery-1.7.1.min.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery.dataTables.min.js" type="text/javascript"></script>
    <script src="../../Scripts/RentalScript/RentalScript.js" type="text/javascript"></script>
    
    <link href="../../Content/themes/base/jquery.dataTables.css" rel="stylesheet" type="text/css" />
    <link href="../../Content/themes/base/demo_table_jui.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" type="text/css" href="../../Content/reset.css" />
    <link rel="stylesheet" type="text/css" href="../../Content/grid.css" />
    <link rel="stylesheet" type="text/css" href="../../Content/icons.css" />
    <link rel="stylesheet" type="text/css" href="../../Content/style.css" />
</head>
<body>
<section class="c12 main-title">
           		<div class="c6 quote">
                	<img src="../../Images/qoute-ion.png" alt="img1" />
                	<p>RentalSpars Form</p>
                    
                     
                </div>
            </section>
    
        @using (Html.BeginForm("RentalSpars", "RentalSpars", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
           <section>
            	<div class="greay_box">
                	<ul>
                    	<li>
        @Html.LabelFor(x => x.RentalSparsName, new { @class = "Label" })
        @Html.TextBoxFor(x => x.RentalSparsName, new { @class = "TextBox" })
        @Html.ValidationMessageFor(x => x.RentalSparsName)
             </li>
                  <li>                                     
        @Html.LabelFor(x => x.Description, new { @class = "Label" })
        @Html.TextBoxFor(x => x.Description, new { @class = "TextBox" })
        @Html.ValidationMessageFor(x => x.Description)
          </li>     
          <li>                                     
        @Html.LabelFor(x => x.Warrenty, new { @class = "Label" })
        @Html.TextBoxFor(x => x.Warrenty, new { @class = "TextBox" })
        @Html.ValidationMessageFor(x => x.Warrenty)
         </li>
         <li>                                            
        @Html.LabelFor(x => x.UnitPrice, new { @class = "Label" })
        @Html.TextBoxFor(x => x.UnitPrice, new { @class = "TextBox" })
        @Html.ValidationMessageFor(x => x.UnitPrice)
             </li>
            <li>                                 
        @Html.LabelFor(x => x.Discount, new { @class = "Label" })
        @Html.TextBoxFor(x => x.Discount, new { @class = "TextBox" })
        @Html.ValidationMessageFor(x => x.Discount)
           </li>  
            
            <li>
            <label >Image</label>
            <input type="file" name="image" />  
            
            </li>                                             
         @Html.ValidationSummary(true)  
          </ul>
                </div>
                 <div class="submit_box">
            	<input type="submit" value="Create Rental Spars" class="Button" />
            </div>
            </section>                                  
                  
               
                                              
        }
    
   
</body>
</html>
